		"theme": "default"
	},

	"roles": {
		"editor": {
			"name": "Editeur",
			"permissions": [ "can-manage-pages" ]
		},

		"admin": {
			"name": "Administrateur",
			"permissions": [ "can-mangage-pages", "can-manage-users",
			"can-manage-models", "can-manage-settings" ]
		},

		"super-admin": {
			"name": "Super-administrateur",
